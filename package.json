{
  "name": "ebac-project",
  "version": "1.0.0",
  "description": "Одностраничный лендинг \"EBAC\"",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=8080",
    "start:dev:server": "npx json-server ./json-server/db.json",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:ts:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kirill-Begej/EBAC-project"
  },
  "keywords": [
    "EBAC",
    "Cursos",
    "webinars",
    "treinamento",
    "aprender o design",
    "profissões contemporâneas"
  ],
  "author": "Kirill Begej",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kirill-Begej/EBAC-project/issues"
  },
  "homepage": "https://github.com/Kirill-Begej/EBAC-project#readme",
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.6",
    "json-server": "^1.0.0-beta.3",
    "lint-staged": "^15.2.10",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.0.9",
    "prettier": "^3.3.3",
    "style-loader": "^4.0.0",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "typescript-plugin-css-modules": "^5.1.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
  }
}
